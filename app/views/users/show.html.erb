<div class="subNavbar">
  <div class="subNavbar-links">
    <ul class="nav subNavbar-links" id="myTab" role="tablist">
      <li>
        <a class="subNavbarLink active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">Profile</a>
      </li>
    </ul>
  </div>
</div>

<div class="container">
  <div class="full-height-profile">
    <div class="tab-content" id="myTabContent">
      <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab"
        >
        <div class="container">
          <div class="full-height-profile">
            <div class="form-card" id="profile-avatar">
              <h2>Welcome to <%= @user.username %>'s profile</h2>
              <%# @equipmen.each do |equipment| %>
              <%= cl_image_tag @user.avatar_photo, width: 100, height: 100, crop: :fill, radius: "max"%>
              <br>
              <%# render "devise/shared/links" %>
            </div>
          </div>
        </div>

      </div>
    </div>
  </div>
</div>
