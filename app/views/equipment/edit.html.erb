<h1>Edit <%= @equipment.name %></h1>
<%= simple_form_for @equipment do |f| %>
  <%= f.input :name %>
  <%= f.input :sport %>
  <%= f.input :description %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Change", class: "btn btn-primary"%>
<% end %>
<%= link_to "Back", equipment_path(@equipment) %>

